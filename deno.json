{
  "tasks": {
    "dev": "deno run -A npm:vite",
    "dev:api": "deno run -A --env-file=.env.local --watch dev-server.ts --port=3000",
    "dev:full": "./start-dev.sh",
    "build": "deno run -A npm:vite build",
    "build:dev": "deno run -A npm:vite build --mode development",
    "preview": "deno run -A npm:vite preview",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "check": "deno check src/",
    "test:token": "deno run -A --env-file=.env.local test-deno-token.ts",
    "test:endpoints": "deno run -A --env-file=.env.local test-endpoints.ts"
  },
  "lint": {
    "include": ["src/"],
    "exclude": ["node_modules/", "dist/"],
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-unused-vars", "no-explicit-any"]
    }
  },
  "fmt": {
    "include": ["src/"],
    "exclude": ["node_modules/", "dist/"],
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": false,
    "semiColons": true
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true
  },
  "imports": {
    "@/": "./src/",
    "@deno/sandbox": "jsr:@deno/sandbox@^0.0.10",
    "@std/http": "jsr:@std/http@^1.0.20",
    "@anthropic-ai/sdk": "npm:@anthropic-ai/sdk@^0.60.0",
    "@hookform/resolvers": "npm:@hookform/resolvers@^3.10.0",
    "@radix-ui/react-accordion": "npm:@radix-ui/react-accordion@^1.2.12",
    "@radix-ui/react-alert-dialog": "npm:@radix-ui/react-alert-dialog@^1.1.15",
    "@radix-ui/react-aspect-ratio": "npm:@radix-ui/react-aspect-ratio@^1.1.7",
    "@radix-ui/react-avatar": "npm:@radix-ui/react-avatar@^1.1.10",
    "@radix-ui/react-checkbox": "npm:@radix-ui/react-checkbox@^1.3.3",
    "@radix-ui/react-collapsible": "npm:@radix-ui/react-collapsible@^1.1.12",
    "@radix-ui/react-context-menu": "npm:@radix-ui/react-context-menu@^2.2.16",
    "@radix-ui/react-dialog": "npm:@radix-ui/react-dialog@^1.1.15",
    "@radix-ui/react-dropdown-menu": "npm:@radix-ui/react-dropdown-menu@^2.1.16",
    "@radix-ui/react-hover-card": "npm:@radix-ui/react-hover-card@^1.1.15",
    "@radix-ui/react-label": "npm:@radix-ui/react-label@^2.1.7",
    "@radix-ui/react-menubar": "npm:@radix-ui/react-menubar@^1.1.16",
    "@radix-ui/react-navigation-menu": "npm:@radix-ui/react-navigation-menu@^1.2.14",
    "@radix-ui/react-popover": "npm:@radix-ui/react-popover@^1.1.15",
    "@radix-ui/react-progress": "npm:@radix-ui/react-progress@^1.1.7",
    "@radix-ui/react-radio-group": "npm:@radix-ui/react-radio-group@^1.3.8",
