    "vaul": "npm:vaul@^0.9.9",
    "zod": "npm:zod@^3.25.76",
    "@types/react": "npm:@types/react@^18.3.23",
    "@types/react-dom": "npm:@types/react-dom@^18.3.7",
    "@types/node": "npm:@types/node@^22.16.5",
    "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^3.11.0",
    "autoprefixer": "npm:autoprefixer@^10.4.21",
